<MileXaml:Application
  x:Class="NanaBox.App"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:MileXaml="using:Mile.Xaml"
  xmlns:local="using:NanaBox">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="MinimalExpanderStyles.xaml"/>
        <MileXaml:XamlControlsResources />
      </ResourceDictionary.MergedDictionaries>
      <local:NegativeDoubleConverter x:Name="NegativeDoubleConverter"/>
    </ResourceDictionary>
  </Application.Resources>
</MileXaml:Application>
